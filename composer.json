{
    "name": "larakitdev/envbar",
    "description": "This package will show on the top of every page a fancy alert with the environment and the current branch information.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "LaraKit\\EnvBar\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaraKit\\EnvBar\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaraKit\\EnvBar\\EnvBarServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "LaraKit",
            "email": "larakit@larakit.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5",
        "guzzlehttp/guzzle": "^7.5",
        "ext-json": "*"
    }
}
